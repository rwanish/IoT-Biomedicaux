    {
        "id": "fa5e50c7198b3e9b",
        "type": "function",
        "z": "f6f852712c49e579",
        "name": "function SpO2",
        "func": "let payload = msg.payload;\n\n// Vérifier que toutes les données sont présentes\nif (payload && payload.timestamp && payload.spo2 !== undefined) {\n    // Convertir le timestamp Unix en millisecondes\n    let date = new Date(payload.timestamp * 1000);  \n    let formattedDate = date.toLocaleString(\"fr-FR\", {\n        year: 'numeric',\n        month: '2-digit',\n        day: '2-digit',\n        hour: '2-digit',\n        minute: '2-digit',\n        second: '2-digit'\n    });\n\n    // Créer le message pour la saturation en oxygène\n   /*let msgSpO2 = {\n        payload: {\n            topic: 'SpO2',\n            value: payload.spo2,\n            timestamp: formattedDate\n        }\n    };*/\n    let msgspo2 = payload.spo2;\n    // Retourner le message\n   msg.payload=msgspo2;\n\n    // Retourner le message\n} else {\n    return { payload: \"Données invalides\" };\n}\n    return msg;\n    \n",
